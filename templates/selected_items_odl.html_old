<html>
	<body>
		Categories
		<br>
		<br>
		{%for category in categories %}
		
		<a href="{{url_for('getItems', category_title=category.title)}}">{{ category.title }}</a>
		<br>
		{% endfor%}

		<br>
		Selected Items
		<br>
		<br>
			{% with messages = get_flashed_messages() %}
			{%if messages: %}
				<ul>
					{%for message in messages%}
					
						<li><strong>{{message}}</strong></li>
					
					{%endfor%}
				
				</ul>
			{%endif%}
		{%endwith%}
		
		<br>
		
		
		{%for item in items %}
		<a href="{{url_for('getItem', category_title = item.category.title, item_title = item.title)}}">{{item.title}}</a>
		<br>
		{%endfor%}

	</body>
</html>
